<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:GestaoPDF.Client.Maui"
             xmlns:lib="clr-namespace:GestaoPDF.Client.Components;assembly=GestaoPDF.Client.Components"
             x:Class="GestaoPDF.Client.Maui.MainPage"
             BackgroundColor="{DynamicResource PageBackgroundColor}">

    <BlazorWebView HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type lib:Main}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>